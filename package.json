{
  "name": "agendabot",
  "version": "0.0.1",
  "description": "Bot para agendamento do The Collective",
  "main": "index.js",
  "scripts": {
    "watch:dev": "npm-run-all --parallel watch start:dev --print-label",
    "start": "tsc -p tsconfig.json && node dist/src/index.js",
