  "license": "GPL-3.0",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.1",
    "@types/node-schedule": "^1.3.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "inversify": "^5.1.1",
    "moment": "^2.29.1",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.13",
    "node-schedule": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "tmi.js": "^1.8.3",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
